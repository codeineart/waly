  "private": true,
  "scripts": {
    "start": "yarn run app",
    "app": "concurrently -k --names \"BACK,FRONT\" -c \"bgBlack.green,bgBlack.green\" \"yarn run back\" \"yarn run front\"",
    "front": "webpack-dev-server --inline --watch --progress --config build/webpack.dev.conf.js",
    "back": "nodemon server/entry.js",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js",
    "debug:chrome": "google-chrome --remote-debugging-port=9229",
    "debug:browser-sync": "browser-sync start --proxy \"localhost:8080\" --files \"./src/**\"",
    "cloud": "ssh ubuntu@app.embodiedreports.com",
    "deploy": "./deployScript.sh",
    "debug": "emulator @nexus  && yarn run debug:browser-sync"
  },
